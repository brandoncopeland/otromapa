<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	  <system.webServer>
		    <staticContent>
		    		<mimeMap fileExtension=".json" mimeType="application/json" />
		    </staticContent>
        <httpProtocol>
            <customHeaders>
                <add name="X-UA-Compatible" value="IE=edge,chrome=1" />
            </customHeaders>
        </httpProtocol>
	  </system.webServer>
</configuration>
